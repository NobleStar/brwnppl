<% for story in @stories.models: %>
  <div class='post'>
      <!-- user avatar-->
      <div class='postUser'><img src='<%= story.get('user')['avatar'] %>'></div>

      <!-- story content -->
      <div class='postContent'>
        <!-- story posted by-->
        <a class='postUsername' href='/#/<%= story.get('user')['username'] %>'><%= story.get('user')['name'] %></a>
        <br>
        <span class="icon posted link"></span>
        <!-- story title -->
        <h2><a href='<%= story.get('url') %>'><%= story.get('title') %></a></h2>

        <div class='clear'></div>

        <!-- story details -->
        <div class='postDetails'>
          
          <!-- Counts -->
          <a href="#"><span class="icon details thumb"></span>255</a>
          <a href="#"><span class="icon details comments"></span>255</a>
          <a href="#"><span class="icon details repost"></span>255</a>
        </div>
      </div>

      <!-- Post thumbnail -->
      <div class='postThumbnail'>
        <img src='<%= story.get('image') %>' max-height='85' max-width='125'>
      </div>

      <div class='clear'></div>

  </div>
<% end %>